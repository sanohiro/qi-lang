; パイプライン演算子のテスト

; 単純なパイプライン
(def inc (fn [x] (+ x 1)))
(print (10 |> inc))  ; 11

; パイプラインのチェーン
(print (1 |> inc |> inc))  ; 3

; 引数ありの関数
(print (10 |> (+ 5)))  ; 15

; 複雑なパイプライン
(print (1 |> (+ 2) |> (* 3)))  ; 9

; 複数ステップのパイプライン
(def double (fn [x] (* x 2)))
(def square (fn [x] (* x x)))
(print (5 |> double |> square))  ; 100
